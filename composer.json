{
    "name": "aternos/poggit-api",
    "type": "library",
    "description": "PHP Client for the Poggit API. This client is based on openapi.",
    "keywords": [
        "poggit",
        "pocketmine",
        "plugins",
        "php",
        "api"
    ],
    "homepage": "https://github.com/aternosorg/php-poggit-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Aternos GmbH",
            "homepage": "https://aternos.gmbh"
        }
    ],
    "require": {
        "php": "^8.3",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.4.5",
        "guzzlehttp/psr7": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.2"
    },
    "autoload": {
        "psr-4": { "Aternos\\PoggitApi\\" : "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Aternos\\PoggitApi\\Test\\" : "tests/" }
    }
}
